CREATE TABLE IF NOT EXISTS `drzava`
(
  `id` INTEGER NOT NULL AUTO_INCREMENT,
  `skracenica2` TEXT NOT NULL,
  `naziv_velikim` TEXT NOT NULL
  `naziv_malim` TEXT NOT NULL,
  `skracenica3` TEXT DEFAULT NULL,
  `numcode` INTEGER DEFAULT NULL,
  `phonecode` INTEGER NOT NULL,
  CONSTRAINT provjera_duzine_skarcenice2 CHECK (lenght(nvl(skracenica2, 'a')) = 2 ),
  CONSTRAINT provjera_duzine_skarcenice3 CHECK (lenght(nvl(skracenica3, 'a')) = 3 ),
   PRIMARY KEY (`id`)
);

CREATE TABLE `pjevac` (
	`id`	INTEGER,
	`ime`	TEXT NOT null,
	`prezime`	TEXT NOT null,
	`datum_rodjenja` INTEGER NOT null,
	CONSTRAINT provjer_godine_rodjenja CHECK(godina_rodjenja > 1960 AND godina_rodjenja < EXTRACT(YEAR FROM sysdate))
	PRIMARY KEY(`id`),
	FOREIGN KEY(`id_drzava`) REFERENCES `drzava`(`id`),
);
-------------------------------------------------
CREATE TABLE IF NOT EXISTS `korisnik` (
	`username`	TEXT, --username jednosznacno odredjuje korisnika, pa se moze koristiti kao id
	`ime`	TEXT NOT null,
	`prezime`	TEXT NOT null,
	`email`	INTEGER DEFAULT null,
	`datum_rodjenja`	TEXT null,
	`id_drzava`	INTEGER NOT null,
	FOREIGN KEY(`id_drzava`) REFERENCES `drzava`(`id`),
	PRIMARY KEY(`username`)
);

